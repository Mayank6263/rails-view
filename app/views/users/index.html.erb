<% if flash[:notice] %>
  <%= flash[:notice] %>
<% end %>
<h1>Users#index</h1>
  <table border="1px">
    <thead>
      <tr>
        
        <th>Name</th>
        <th colspan="2">Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, user_path(user)%></td>
        <td><%= link_to "Edit", edit_user_path(user) %></td>
        <td><%= link_to "Destroy"%></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <br>
  <%= link_to "New User", new_user_path %>
