<h1>Users#index</h1>
  <table>
    <thead>
      <tr>
        <th>Id</th>
        <th>Name</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <br>
  <%= link_to "New User", new_user_path %>
